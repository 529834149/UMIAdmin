{
    "name": "ym/umi",
    "description": "umi is powerful laravel admin BREAD and RBAC system, just few minutes to click button and you will see the magic world",
    "keywords": ["laravel", "admin", "bread", "rbac" , "backend"],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Tao Yu",
            "email": "taoyu65@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "psr-4": {
            "YM\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev"
}
